{
  "name": "chaap",
  "version": "0.1.0",
  "description": "naisho",
  "main": "index.js",
  "scripts": {
    "dep": "goapp deploy app/app.yaml",
    "build": "npm-run-all -p minify:*",
    "dev": "npm-run-all -p watch:*",
    "watch:js": "watchify -o src/bundle.js -v -d src/index.js",
    "minify:js": "uglifyjs --compress --mangle --output dist/bundle.js -- src/bundle.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dovneko/chaap.git"
  },
  "keywords": [
    "chaappp"
  ],
  "author": "NekoKuma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dovneko/chaap/issues"
  },
  "homepage": "https://github.com/dovneko/chaap#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "npm-run-all": "^2.3.0",
    "riotify": "^1.0.1",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "riot": "^2.5.0"
  },
  "browserify": {
    "transform": [
      [
        "riotify",
        {
          "type": "es6"
        }
      ]
    ]
  }
}
